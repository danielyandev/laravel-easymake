{
    "name": "danielyandev/laravel-easymake",
    "description": "Easy make commands for Laravel",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Ruben Danielyan",
            "email": "rub1994.13@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "laravel/framework": "~5.5|~6.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "EasyMake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasyMake\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "EasyMake\\EasyMakeServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    }
}
